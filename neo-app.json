{
  "welcomeFile": "/index.html",
  "routes": [
    {
      "path": "/sap/opu/odata",
      "target": {
        "type": "destination",
        "name": "CN4",
        "entryPath": "/sap/opu/odata"
      },
      "description": "CN4"
    },
    {
      "path": "/customtheme",
      "target": {
        "type": "application",
        "name": "theme"
      },
      "description": "Custom theme folder"
    },
    {
      "path": "/sap/bc/ui5_ui5",
      "target": {
        "type": "destination",
        "name": "CN4",
        "entryPath": "/sap/bc/ui5_ui5"
      },
      "description": "CN4"
    },
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "**.html"
    },
    {
      "directive": "private",
      "maxAge": 0,
      "path": "sap-ui-cachebuster-info.json"
    },
    {
      "directive": "public",
      "maxAge": 31536000
    }
  ]
}